    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>bc1194bb-7562-4c50-acaa-256aca39be10</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="7.0.17" />
